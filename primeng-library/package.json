{
  "name": "clean-minds-primeng-library",
  "description": "PrimeNg App Library from Clean Minds way",
  "author": "The clean minds coaching group",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --aot --preserve-symlinks",
    "start": "npm run serve",
    "test": "ng test --app=library --preserve-symlinks",
    "test:integration": "ng test --preserve-symlinks",
    "coverage": "ng test --app=library --code-coverage --single-run --preserve-symlinks",
    "coverage:integration": "ng test --code-coverage --single-run --preserve-symlinks",
    "e2e": "ng e2e --preserve-symlinks",
    "lint": "ng lint --type-check true",
    "doc": "typedoc --mode file --module umd --target ES5 --exclude \"**/*+(spec|test|e2e|po).ts\" --excludePrivate --hideGenerator --name \"The Clean Minds PrimeNg Library\" --readme ./README.md --out doc library",
    "build": "ng-packagr -p library/package.json && cd dist/clean-minds-primeng-library && npm pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "primeng": "^5.2.6",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.4.4",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typedoc": "^0.11.1",
    "typescript": "~2.5.3"
  }
}
